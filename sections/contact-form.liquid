{{ 'section-contact-form.css' | asset_url | stylesheet_tag }}

{%- style -%}
  .section-{{ section.id }}-padding {
    padding-top: {{ section.settings.padding_top | times: 0.75 | round: 0 }}px;
    padding-bottom: {{ section.settings.padding_bottom | times: 0.75 | round: 0 }}px;
  }

  @media screen and (min-width: 750px) {
    .section-{{ section.id }}-padding {
      padding-top: {{ section.settings.padding_top }}px;
      padding-bottom: {{ section.settings.padding_bottom }}px;
    }
  }
  .section-{{ section.id }}-padding h3.form-labels {
      font-weight: 600;
      margin-bottom: 8px;
      color: {{ section.settings.label_color }};
  }
  .section-{{ section.id }}-padding .inline-richtext {
    font-weight: 700;
    font-size: 20px;
    margin-bottom: 10px;
    @media(min-width: 768px) {
      font-size: 28px;
      margin-bottom: 16px;
    }
  }
  .section-{{ section.id }}-padding .sub-title {
    margin-bottom: 24px;
    margin-top: 10px;
    font-size: 16px;
    line-height: 1.5;
    @media(max-width: 768px) {
      margin-bottom: 16px;
      font-size: 12px;
    }
  }
  .section-{{ section.id }}-padding .custom-button {
    background-color: {{ section.settings["btn-background-color"] }};
    color: {{ section.settings["btn-color"] }};
    padding: 1rem 3.5rem;
    border-radius: 4px;
    cursor: pointer;
  }
  .section-{{ section.id }}-padding {
    @media(max-width: 992px) {
      padding-bottom: 0;
    }
  }
   .section-{{ section.id }} {
        width: 100%;
        margin: 0 auto;
        padding-inline: 10px;
        @media (width >= 576px) {
            max-width: 540px;
        }
        @media (width >= 768px) {
            max-width: 720px;
        }
        @media (width >= 992px) {
            max-width: 960px;
        }
        @media (width >= 1200px) {
            max-width: 1140px;
        }
        @media (width >= 1400px) {
            max-width: 1320px;
        }
        display: grid;
        grid-template-columns: repeat(3, minmax(0, 1fr));
        gap: 0;
        @media(max-width: 1250px) {
          gap: 40px;
        }
        @media(max-width: 992px) {
          grid-template-columns: repeat(1, minmax(0, 1fr));
          gap: 24px;
        }
   }
  .section-{{ section.id }} .map-section::before {
    content: "";
    background: url({{ section.settings.map_background_image | image_url: width: 1100 }}) no-repeat right bottom;
    opacity: 0.2;
    position: absolute;
    inset: 0;
  }
  .section-{{ section.id }} .map-section {
     order: 1;
      position: relative;
     padding: 16px;
     display: grid;
     gap: 24px;
     border-radius: 4px;
      background-color: {{ section.settings.map_background }};
     margin-top: 36px;
     @media (max-width: 992px) {
         margin-top: 0;
     }
  }
  .section-{{ section.id }} .map-section .map-iframe {
    min-height: 270px;
    border-radius: 4px;
  }
  .section-{{ section.id }} .map-section .map-info {
       display: flex;
       flex-direction: column;
       gap: 24px;
       position: relative;
       z-index: 2;
  }
  .section-{{ section.id }} .map-section .map-info-content {
       display: flex;
       gap: 10px;
  }
  .section-{{ section.id }} .map-section .map-info-heading {
       color: {{ section.settings.info_color  }};
       font-weight: 600;
       display: flex;
       align-items: center;
       gap: 8px;
       font-size: 20px;
       @media (max-width: 768px) {
           font-size: 16px;
       }
   }
   .section-{{ section.id }} .map-section .map-info-text {
       color: {{ section.settings.info_color  }};
       opacity: 0.8;
       font-size: 16px;
       @media (max-width: 768px) {
           font-size: 12px;
       }
  }
   .section-{{ section.id }} .map-section .map-info_contact {
       display: flex;
       flex-direction: column;
       align-items: center;
  }
  .section-{{ section.id }} .map-section .map-info-icon {
       display: flex;
       align-items: start;
        padding-top: 4px;
  }
  .section-{{ section.id }} .contact-section {
      display: flex;
      flex-direction: column;
      width: 100% !important;
      padding-bottom: 0;
       @media(min-width: 992px) {
           grid-column: span 2;
           margin-bottom: 0;
          }
   }
  .section-{{ section.id }} .contact-section form {
    display: flex;
    flex-direction: column;
    height: 100%;
    @media (min-width: 1250px) {
      max-width: 93%;
    }
  }
   .section-{{ section.id }} .field::after {
       box-shadow: none;
   }
   .section-{{ section.id }} .field__input {
       background-color: #F3F3F3;
       border-radius: 4px;
       border: 1px solid #ddd;
   }
   .section-{{ section.id }} .field__input:focus {
       box-shadow: none;
   }
   .section-{{ section.id }} .form-status {
       margin-bottom: 16px !important;
       line-height: 1.5;
    }
    .section-{{ section.id }} .map-info_contact__social {
        display: flex;
        gap: 16px;
    }
{%- endstyle -%}

